/obj/item/melee/telebaton
	name = "телескопическая дубинка"
	desc = "Компактное, но сбалансированное оружие индивидуальной защиты. В сложенном состоянии может быть скрыто."

/obj/item/tableflag/rainbow
	name = "радужный флажок"
	desc = "Флаг в цветах радуги, прославляющий инклюзивность, разнообразие и равенство во всей Вселенной."

/obj/item/tape_roll
